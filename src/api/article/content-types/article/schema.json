{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Etudiant",
    "description": "Create your blog content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nom": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "annee_entree": {
      "type": "string"
    },
    "niveau_actuel": {
      "type": "integer",
      "max": 3,
      "default": 1
    },
    "profile_photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "pwd": {
      "type": "password"
    }
  }
}
